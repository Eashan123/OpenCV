   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "image = cv2.imread('./Images/one.jpg')\n",
    "height, width = image.shape[:2]\n",
    "\n",
    "cropped_image = image[150:500, 150:600]\n",
    "\n",
    "cv2.imshow('Cropped Image', cropped_image)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
